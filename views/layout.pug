    script.
      $(".js-show-modal1").on("click", function (){
        let _id = $(this).data("id");
        $.ajax({
          url: "/product/product-detail-quick",
          type: "POST",
          data:{
            id: _id
          },
          success: (result) => {
            $("#modal-body-product").stop().animate(0, function(){
              $("#modal-body-product .slick3").html(result.product.imagePath.map((res)=>{
                return `<div class="item-slick3" data-thumb=${res}>
                          <div class="wrap-pic-w pos-relative">
                              <img src=${res} alt="IMG-PRODUCT">
                              <a class="flex-c-m size-108 how-pos1 bor0 fs-16 cl10 bg0 hov-btn3 trans-04" href=${res}>
                                  <i class="fa fa-expand"></i>
                              </a>
                          </div>
                      </div>`;
              }));
              $("#modal-name-product").html(`${result.product.productName}`);
              $("#modal-price-product").html(`$ ${result.product.price}`);
              $("#modal-des-product").html(`${result.product.description}`);
              $("#id").val(`${result.product._id}`);
              $("#modal-select-size").html(result.product.details.map((res) => { 
                return `<option value='${res.size}'>${res.size}</option>`
                    }));
              $("#modal-select-color").html(result.product.details.map((res) => {
                        return `<option value='${res.color}'>${res.color}</option>`
                    }));
            })
          }
        })
      });
    script.
      $(".js-addcart-detail").on("click",function(){
        const id = $(".id-detail").val();
        const size = $(".size-detail").val();
        const color = $(".color-detail").val();
        const quantity = $(".quantity-detail").val();
        $.post("/shoping-cart/addToCart",{id,size,color,quantity})
      })
    script.
      let count = 0;
      let totalItemDelete = 0;
      $(".table-shopping-cart .table_row").each(function(){
        count++;
        $(this).find(".how-itemcart1").on("click", function(){
          let id = $(this).find("img").attr("data-id");
          $.get("/shoping-cart/deleteItemInCart", {id});
          $(this).parent().parent().css("display","none");
          $(".header-cart-wrapitem li").each(function(){
            let id2 = $(this).find(".header-cart-item-img img").attr("data-id");
            if(id === id2)
            {
              totalItemDelete = parseFloat($(this).find(".price-product-view-cart").html().replace(/[^0-9.]/g, "")) * parseInt($(".quantity-view-cart").html().replace(/[^0-9.]/g, ""));
              $(this).css("display","none");
            };
          });
          count -= 1;
          $(".wrap-menu-desktop .js-show-cart").attr("data-notify", count);
          $(".wrap-header-mobile .js-show-cart").attr("data-notify", count);
          $(".total-price").html();
          let total = parseFloat($(".total-price").html().replace(/[^0-9.]/g, ""));
          total = total - totalItemDelete;
          $(".total-price").html(`$ ${total}`);
        });
      });

