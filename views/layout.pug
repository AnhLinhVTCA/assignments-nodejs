    script.
      let count1 = 0;
      let totalItemDelete1 = 0;
      $(".header-cart-wrapitem li").each(function(){
        count1++;
        $(this).find(".header-cart-item-img").on("click", function(){
          let id = $(this).find("img").attr("data-id");
          $.get("/shoping-cart/deleteItemInCart", {id});
          $(this).parent().css("display","none");
          $(".table-shopping-cart .table_row").each(function(){
            let id2 = $(this).find("img").attr("data-id");
            if(id === id2)
            {
              $(this).css("display","none");
            };
          });
          count1 -= 1;
          $(".wrap-menu-desktop .js-show-cart").attr("data-notify", count1);
          $(".wrap-header-mobile .js-show-cart").attr("data-notify", count1);
          totalItemDelete1 = parseFloat($(this).parent().find(".price-product-view-cart").html().replace(/[^0-9.]/g, "")) * parseInt($(".quantity-view-cart").html().replace(/[^0-9.]/g, ""));
          let total = parseFloat($(".total-price").html().replace(/[^0-9.]/g, ""));
          total = total - totalItemDelete1;
          $(".total-price").html(`$ ${total}`);
        });
      });
